#include <stdio.h>
#include <stdlib.h>

int main () {
	int numero1, numero2;
	
	printf("Maior de dois numeros \n");
	printf("Digite o primeiro numero: \n");
	scanf("%i", &numero1);
	printf("Digite o segundo numero: \n");
	scanf("%i", &numero2);
	
	if (numero1==numero2) {
		printf("Valor igual \n");
	} else {
		if (numero1>numero2) {
			printf("O maior numero e: %i", numero1);
		} if (numero1<numero2) {
			printf("O maior numero e: %i", numero2);
		}
	}
	
	return 0;
}
